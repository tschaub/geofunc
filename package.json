    "test-watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "start": "jest --watch"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.4",
    "@types/geojson": "^7946.0.10",
    "eslint": "^8.48.0",
    "eslint-config-tschaub": "^14.1.2",
    "jest": "^29.6.4",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "extends": "tschaub",
    "settings": {
      "jsdoc": {
        "mode": "typescript",
        "preferredTypes": {
          "[]": "Array<>",
          ".<>": "<>",
          "object": "Object"
        }
      }
    }
  }
