{
  "name": "geofunc",
  "version": "0.3.0",
  "main": "main.js",
  "files": [
    "main.js"
  ],
  "type": "module",
  "scripts": {
    "lint": "eslint *.js",
    "typecheck": "tsc --pretty",
    "pretest": "npm run lint && npm run typecheck",
